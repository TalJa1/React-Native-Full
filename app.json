{
  "expo": {
    "name": "react-full-app",
    "slug": "react-full-app",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow react-full-app to access your camera",
          "microphonePermission": "Allow react-full-app to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#E6A3D9",
          "defaultChannel": "default",
          "sounds": ["./local/assets/mainSound.wav"]
        }
      ]
    ]
  },
  "android": {
    "useNextNotificationsApi": true,
    "notification": {
      "icon": "./local/assets/notification-icon.png",
      "color": "#E6A3D9",
      "defaultChannelId": "default"
    }
  },
  "ios": {
    "supportsTablet": true,
    "bundleIdentifier": "com.yourcompany.yourapp",
    "buildNumber": "1.0.0",
    "notification": {
      "sound": true
    }
  },
  "name": "react-full-app"
}
