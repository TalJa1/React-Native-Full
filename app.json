{
  "expo": {
    "name": "react-full-app",
    "slug": "react-full-app",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow react-full-app to access your camera",
          "microphonePermission": "Allow react-full-app to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            
          ]
        }
      ]
    ]
  },
  "name": "react-full-app"
}
